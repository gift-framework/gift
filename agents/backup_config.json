{
  "retention_days": {
    "daily": 30,
    "weekly": 90,
    "monthly": 365
  },
  "backup_schedule": {
    "daily": "02:00",
    "weekly": "sunday@02:00",
    "monthly": "1@02:00"
  },
  "include_patterns": [
    "github/**",
    "new_work/**",
    "wip_research/**",
    "agents/**",
    "README.md"
  ],
  "exclude_patterns": [
    "legacy/**",
    "backups/**",
    "**/__pycache__/**",
    "**/*.pyc",
    "**/.git/**"
  ],
  "canonical_docs_only": false,
  "compression": true,
  "encryption": false
}